{
  "author": "Troy Alford",
  "bugs": "https://github.com/TroyAlford/react-jsx-parser/issues",
  "description": "A React component which can parse JSX and output rendered React Components",
  "keywords": [
    "react",
    "jsx"
  ],
  "license": "MIT",
  "main": "lib/react-jsx-parser.min.js",
  "name": "react-jsx-parser",
  "repository": "TroyAlford/react-jsx-parser",
  "version": "1.21.0",
  "dependencies": {
    "acorn": "^7.1.0",
    "acorn-jsx": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-classes": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "mkdirp": "^0.5.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "source-map-explorer": "^2.1.0",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9"
  },
  "peerDependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "resolutions": {
    "extend": "^3.0.2",
    "merge": "^1.2.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "postversion": "git push origin HEAD && git push origin HEAD --tags",
    "prebuild": "mkdirp ./lib && rm -rf ./lib/*",
    "preversion": "yarn test",
    "sourcemap": "yarn build && source-map-explorer ./lib/react-jsx-parser.min.js",
    "test": "yarn jest",
    "version": "yarn build && git add -A ./lib"
  },
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>setupTests.js"
    ],
    "transform": {
      ".js$": "babel-jest"
    }
  }
}
